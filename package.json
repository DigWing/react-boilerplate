{
  "name": "react-boilerplate",
  "author": "Digital Wing",
  "version": "0.3.0",
  "keywords": [
    "react",
    "boilerplate",
    "digital wing"
  ],
  "description": "React boilerplate, based on Digital Wing usual project architecture",
  "repository": {
    "type": "git",
    "url": "https://github.com/DigWing/react-boilerplate.git"
  },
  "dependencies": {
    "@digitalwing.co/redux-query-immutable": "2.4.13",
    "classnames": "^2.2.6",
    "history": "^4.9.0",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.19",
    "normalizr": "^3.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-app-polyfill": "^0.2.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-form": "^8.2.4",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@svgr/webpack": "4.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "1.0.0",
    "dotenv": "8.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "6.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "^3.11.1",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "4.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "8.0.1",
    "git-revision-webpack-plugin": "^3.0.3",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "husky": "^3.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "^24.8.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "lint-staged": "^9.2.1",
    "mini-css-extract-plugin": "0.7.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.3.1",
    "postcss-safe-parser": "4.0.1",
    "react-dev-utils": "^7.0.5",
    "resolve": "1.8.1",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "webpack": "^4.41.0",
    "webpack-dev-server": "3.7.2",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.3.1"
  },
  "scripts": {
    "eslint": "eslint --ext .jsx,.js src/",
    "stylelint": "stylelint \"src/**/*.{css,scss,sass}\"",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js"
  },
  "eslintConfig": {
    "plugins": [
      "react-hooks"
    ],
    "extends": [
      "react-app",
      "airbnb"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ]
        }
      }
    },
    "rules": {
      "jsx-a11y/label-has-associated-control": "off",
      "jsx-a11y/label-has-for": "off",
      "react/jsx-filename-extension": "off",
      "import/prefer-default-export": "warn",
      "implicit-arrow-linebreak": "off",
      "react/forbid-prop-types": "off",
      "no-nested-ternary": "warn",
      "import/no-unresolved": "warn",
      "no-throw-literal": "off"
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "at-rule-empty-line-before": null,
      "font-family-no-missing-generic-family-keyword": null
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "lodash",
        {
          "id": [
            "lodash"
          ]
        }
      ]
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(js|jsx)": [
      "eslint",
      "git add"
    ],
    "src/**/*.(css|sass|scss)": [
      "stylelint",
      "git add"
    ]
  }
}
